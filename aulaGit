git init
git --global config user.email "email"
git --global config user.name "nome"
git add
git commit -m 
git remote add origin nome_da_pasta
git push -u origin master

git branch nome_da_branch "cria uma nova branch no repositorio local 
git checkout -b nome_da_branch "cria uma nova branch e troca a referencia para a nova branch

git merge nome_da_branch "mescla altera√ßoes da branch atual com a branch em que foi passsado o nome"
git push para salvar a merge